                                                           class="form-control form-control-solid form-control-lg"
                                                           name="Area"
                                                           placeholder="Powierzchnia apartamentu"
                                                           asp-for="Area"
                                                           oninput="sendToFinalStep(this)" />
                                                    <span class="form-text text-muted">
                                                        Wpisz powierzchnie apartamentu (w m<sup>2</sup>).
                                                    </span>
                                                </div>
                                                <!--end::Input-->


                                            </div>

                                            <div class="col-xl-6">
                                                <!--begin::Input-->
                                                <div class="form-group">
                                                    <label>Piętro</label>
                                                    <input type="text"
                                                           class="form-control form-control-solid form-control-lg"
                                                           name="Floor" placeholder="Piętro"
                                                           asp-for="Floor"
                                                           oninput="sendToFinalStep(this)" />
                                                    <span class="form-text text-muted">
                                                        Wpisz na którym piętrze znajduje się apartament
                                                    </span>
                                                </div>
                                                <!--end::Input-->
