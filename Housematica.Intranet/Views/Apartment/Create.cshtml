                                                           class="form-control form-control-solid form-control-lg" id="ApartmentName"
                                                           name="ApartmentName"
                                                           asp-for="ApartmentName"
                                                           placeholder="Numer/nazwa apartamentu"
                                                           oninput="sendToFinalStep(this)" />
                                                    <span class="form-text text-muted">
                                                        Wpisz nazwę lub numer apartementu
                                                    </span>
                                                </div>
                                                <!--end::Input-->


                                            </div>

                                            <div class="col-xl-6">
                                                <!--begin::Input-->
