                                                    </label>
                                                    <select name="IdProjects"
                                                            id="ProjectName"
                                                            class="form-control form-control-solid form-control-lg"
                                                            oninput="sendToFinalStep(this)"
                                                            asp-for="IdProjects" asp-items="ViewBag.IdProjects">
                                                    </select>


                                                    <span class="form-text text-muted">
                                                        Wybierz projekt
                                                    </span>
                                                </div>
                                                <!--end::Input-->
                                            </div>
                                        </div>

