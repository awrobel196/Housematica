                                                               class="form-control form-control-solid form-control-lg"
                                                               asp-for="ApartmentVariants[0].Rooms[0].RoomArea"
                                                               placeholder="Liczba pomieszczeń" id="1"
                                                               onwheel="return false;"
                                                               onkeyup="addRooms(this.id)"
                                                               onclick="addRooms(this.id)"
                                                               oninput="sendToFinalStep(this)" />
                                                        <span class="form-text text-muted">
                                                            Wpisz liczbę pomieszczeń apartamentu
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group">
                                                        <label>Liczba pokoi</label>
                                                        <input type="number"
                                                               id="numberLivingRoom roomNumber"
                                                               min="0" ;
                                                               onwheel="return false;"
                                                               class="form-control form-control-solid form-control-lg"
                                                               asp-for="ApartmentVariants[0].NumberOfLivingRoom"
                                                               placeholder="Liczba pokoi" value=""
                                                               oninput="sendToFinalStep(this)" />
                                                        <span class="form-text text-muted">
                                                            Wpisz liczbę pokoi apartamentu
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
