                                                </div>
                                                <div>City: <span id="projectCityFinal"></span></div>
                                                <div>State: <span id="projectStateFinal"></span></div>
                                                <div>
                                                    Country: <span id="projectCountryFinal"></span>
                                                </div>

                                            </div>
                                            <div class="separator separator-dashed my-5"></div>
                                            <!--end::Section-->
                                            <!--begin::Section-->
                                            <h6 class="font-weight-bolder mb-3">
                                                Inwetment details:
                                            </h6>
                                            <div class="text-dark-50 line-height-lg">
                                                <div>Project name: <span id="projectNameFinal"></span></div>
                                                <div>Type: <span id="projectTypeFinal"></span></div>
                                                <div>
                                                    Description: <span id="projectDescriptionFinal"></span>
                                                </div>
                                            </div>
                                            <div class="separator separator-dashed my-5"></div>
                                            <!--end::Section-->


                                        </div>
                                        <!--end: Wizard Step 6-->
                                        <!--begin: Wizard Actions-->
                                        <div class="d-flex justify-content-between border-top mt-5 pt-10">
                                            <div class="mr-2">
                                                <button type="button"
                                                        class="btn btn-light-primary font-weight-bold text-uppercase px-9 py-4"
                                                        data-wizard-type="action-prev">
                                                    Previous
                                                </button>
                                            </div>
                                            <div>
                                                <button type="submit"
                                                        value="Save"
                                                        onclick="check()"
