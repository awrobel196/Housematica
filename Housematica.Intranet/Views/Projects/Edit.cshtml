    <script>
        function check() {
            Swal.fire({
                title: "Auto close alert!",


                onOpen: function () {
                    Swal.showLoading()
                }
            }).then(function (result) {
                if (result.dismiss === "timer") {
                    Swal.fire("Good job!", "You clicked the button!", "success");
                    console.log("I was closed by the timer")
                }
            })
        }
    </script>



